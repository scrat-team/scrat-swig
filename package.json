{
  "name": "scrat-swig",
  "version": "0.10.0",
  "description": "Extensions of swig for scrat",
  "main": "index.js",
  "scripts": {
    "lint": "eslint lib test index.js",
    "lint-fix": "eslint --fix .",
    "test": "npm run lint && npm run test-local",
    "test-local": "cross-env TEST_REPORTER=spec NODE_ENV=test ./node_modules/.bin/_mocha --recursive test --bail",
    "cov": "cross-env TEST_REPORTER=spec NODE_ENV=test node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive test --bail",
    "ci": "npm run lint && npm run cov",
    "ci-travis": "npm run lint && TEST_REPORTER=spec NODE_ENV=test node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- --recursive test --bail"
  },
  "dependencies": {
    "swig": "1.4.2"
  },
  "devDependencies": {
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.0.0",
    "cross-env": "^5.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.1",
    "mocha": "^2.2.4",
    "mockjs": "^0.1.4",
    "once": "^1.3.2",
    "pedding": "^1.0.0",
    "rimraf": "^2.3.3",
    "sinon": "^1.14.1",
    "supertest": "^1.1.0"
  },
  "author": "fouber <fouber.NET@gmail.com>",
  "homepage": "https://github.com/scrat-team/scrat-swig",
  "repository": {
    "type": "git",
    "url": "https://github.com/scrat-team/scrat-swig.git"
  },
  "keywords": [
    "pagelet",
    "quickling",
    "scrat",
    "swig",
    "fis"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scrat-team/scrat-swig/issues"
  }
}
